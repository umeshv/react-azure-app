{
  "routes": [
    {
      "route": "/api/*",
      "methods": [
        "get",
        "post"
      ],
      "rewrite": "https://reqres.in/api/{*}",
      "allowedRoles": [
        "anonymous"
      ]
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html"
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html",
      "statusCode": 200
    }
  },
  "mimeTypes": {
    ".json": "text/json"
  },
  "globalHeaders": {
    "Cache-Control": "no-cache"
  }
}